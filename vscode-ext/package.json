{
  "name": "nais-tester",
  "displayName": "tester",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "scripts": {
    "package": "vsce package --allow-missing-repository"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tester",
        "aliases": [
          "tester",
          "tester"
        ],
        "extensions": [
          ".rest.test",
          ".pubsub.test"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "tester.sql",
        "aliases": [
          "tester.sql",
          "tester.sql"
        ],
        "extensions": [
          ".sql.test"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "tester.gql",
        "aliases": [
          "tester.gql",
          "tester.gql"
        ],
        "extensions": [
          ".gql.test"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tester",
        "scopeName": "text.tester",
        "path": "./syntaxes/tester.tmLanguage.json"
      },
      {
        "language": "tester.sql",
        "scopeName": "source.tester.sql",
        "path": "./syntaxes/tester-sql.tmLanguage.json"
      },
      {
        "language": "tester.gql",
        "scopeName": "source.tester.gql",
        "path": "./syntaxes/tester-gql.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.26.0"
  }
}
